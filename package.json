{
  "name": "rn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "updateApp": "yarn && cd ios && pod install && cd ..",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "android": "react-native run-android",
    "ios5": "react-native run-ios --simulator='iPhone 5s'",
    "iosX": "react-native run-ios --simulator='iPhone X'",
    "iosXs": "react-native run-ios --simulator='iPhone XS'",
    "ios": "react-native run-ios",
    "build-android": "cd ./android && ./gradlew app:assembleRelease",
    "build-ios": "react-native run-ios --configuration Release",
    "doc": "jsdoc -p -r ./src/ && jsdoc -r ./src/",
    "eslint": "yarn add eslint && yarn add prettier && yarn add eslint-config-airbnb && yarn add eslint-config-prettier && yarn add eslint-plugin-import && yarn add eslint-plugin-jsx-a11y && yarn add eslint-plugin-prettier && yarn add eslint-plugin-react"
  },
  "dependencies": {
    "react": "16.6.3",
    "react-native": "0.58.5",
    "react-native-smart-splash-screen": "2.3.5",
    "react-navigation": "1.0.3",
    "react-navigation-redux-helpers": "1.0.1",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-form": "^8.1.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "24.1.0",
    "metro-react-native-babel-preset": "0.52.0",
    "prettier": "^1.16.4",
    "react-test-renderer": "16.6.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
